variant: fcos
version: 1.1.0
passwd:
  users:
    - name: core
      ssh_authorized_keys:
        - ssh-rsa ssh-rsa AAAAB3NzaC1yc2EA+++++++++++++++JUST_AN_EXAMPLE+++REPLACE_WITH_ANOTHER_SSH_ example@laptop
storage:
